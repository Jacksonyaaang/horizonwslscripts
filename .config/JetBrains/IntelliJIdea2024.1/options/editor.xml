<application>
  <component name="CodeInsightSettings">
    <option name="completionCaseSensitive" value="2" />
    <option name="COMPLETION_CASE_SENSITIVE" value="2" />
    <option name="ENABLE_SECOND_REFORMAT" value="true" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="Horizon Software">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="EditorSettings">
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_PERSISTENT" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern useCustomColors="true" pattern="\bdel\b.*">
      <option name="FOREGROUND" value="a2fdff" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="//test.*">
      <option name="FOREGROUND" value="f8fff9" />
      <option name="FONT_TYPE" value="1" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>